designated =>
(
	identifier "com.github.GitHubClient"
	or
	identifier "com.github.GitHubClient.helper"
)
and
/* Signed by Apple for App Store Distribution */
(anchor apple generic and certificate leaf[field.1.2.840.113635.100.6.1.9] /* exists */)
or
/* Signed by GitHub for Developer ID Distribution */
(anchor apple generic and certificate 1[field.1.2.840.113635.100.6.2.6] /* exists */ and certificate leaf[field.1.2.840.113635.100.6.1.13] /* exists */ and certificate leaf[subject.OU] = VEKTX9H2N7)
