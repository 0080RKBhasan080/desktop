{
  "name": "desktop",
  "repository": {
    "type": "git",
    "url": "https://github.com/desktop/desktop.git"
  },
  "description": "The People's Glorious GitHub Client",
  "scripts": {
    "test": "electron-mocha --renderer --require ts-node/register test/*.ts test/*.tsx",
    "postinstall": "cd app && typings install",
    "start-server": "node dev_server.js",
    "start": "npm run package && npm-run-all --parallel dev start-server",
    "dev": "node script/run",
    "build": "tsc -p ./app && webpack && cp -R app/static build && cp app/package.json build",
    "clean": "rm -rf build",
    "lint": "tslint app/src/**/*.ts app/src/**/*.tsx",
    "package": "npm run build && node script/package"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "electron-mocha": "1.2.0",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "^0.37.8",
    "express": "^4.13.4",
    "fs-extra": "^0.30.0",
    "mocha": "^2.4.5",
    "npm-run-all": "^1.8.0",
    "react": "^15.0.2",
    "react-addons-test-utils": "^15.0.2",
    "react-transform-hmr": "^1.0.4",
    "ts-loader": "^0.8.2",
    "ts-node": "^0.7.2",
    "tslint": "^3.9.0",
    "typescript": "^1.8.10",
    "typings": "^0.7.12",
    "webpack": "^1.12.15",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-target-electron-renderer": "^0.4.0"
  }
}
